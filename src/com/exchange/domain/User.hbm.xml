<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping 
	package="com.exchange.domain">
	<class name="User" table="Users">
		<id name="id">
			<generator class="uuid"/>
		</id>
		<property name="userName" length="50"/>
		<property name="passWord" length="50"/>	
		<property name="accountNonExpired" length="1"/>	
		<property name="accountNonLocked" length="1"/>	
		<property name="credentialsNonExpired" length="1"/>	
		<property name="enabled" length="1"/>	
	</class>
</hibernate-mapping>